{
  "permissions": {
    "defaultMode": "acceptEdits",
    "allow": [
      "Bash(ls:*)",
      "Bash(cat:*)",
      "Bash(npm run dev)",
      "Bash(npm run build)",
      "Bash(npm test)",
      "Edit",
      "Write",
      "Bash"
    ],
    "deny": ["Bash(rm -rf:*)", "Bash(sudo:*)", "Read(.env)"]
  }
}
